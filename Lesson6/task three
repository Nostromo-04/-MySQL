-- 1. из таблицы users отбираем 10 самых молодых пользователей
USE vk;
SELECT * FROM users;
SELECT id, birthday FROM users ORDER BY birthday DESC LIMIT 10;

id|birthday  |
--+----------+
23|2021-02-10|
61|2020-07-27|
89|2019-09-26|
29|2018-05-24|
77|2018-03-18|
67|2017-04-07|
26|2017-04-01|
46|2016-08-26|
78|2016-07-08|
99|2015-10-04|

-- 23,26,29,46,61,67,77,78,89,99 id young users

-- 2. заполняем таблицы значениями
SELECT * FROM target_types;
INSERT INTO target_types VALUES (1,'media',NOW(),NOW()),(2,'post',NOW(),NOW()),(3,'user',NOW(),NOW()); -- id 3 лайк пользователю, по нему будем считать лайки

id|name |created_at         |updated_at         |
--+-----+-------------------+-------------------+
 1|media|2021-09-13 19:41:43|2021-09-13 19:41:43|
 2|post |2021-09-13 19:41:43|2021-09-13 19:41:43|
 3|user |2021-09-13 19:41:43|2021-09-13 19:41:43|

SELECT * FROM likes;
INSERT INTO likes VALUES (100, floor((1 + rand() * 99)), 99, floor((1 + rand() * 3)), NOW(), NOW()); -- подставляя в третье значение поочередно id young users, заполнил таблицу 

id |user_id|target_id|target_type_id|created_at         |updated_at         |
